      },
      "source": [
        "number=int(input(\"Enter the number\"))\n",
        "\n",
        "\n",
        "for i in range(1,13):\n",
        "\n",
        "  num=number*i\n",
        "\n",
        "  print(number,\"*\", i, \"=\", num)"
      ],
